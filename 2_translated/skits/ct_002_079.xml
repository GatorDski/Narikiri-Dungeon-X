    <Entry>
      <PointerOffset>38452</PointerOffset>
      <VoiceId>&lt;VCT_002_079_002&gt;</VoiceId>
      <JapaneseText>わたし、優しい、充分</JapaneseText>
      <EnglishText>I...kind...enough...</EnglishText>
      <Notes/>
      <Id>2</Id>
      <SpeakerId>2</SpeakerId>
      <Status>Editing</Status>
    </Entry>
    <Entry>
      <PointerOffset>38860</PointerOffset>
      <VoiceId>&lt;VCT_002_079_003&gt;</VoiceId>
      <JapaneseText>ふたり、ケンカ、やめる</JapaneseText>
      <EnglishText>You...fight...stop...</EnglishText>
      <Notes/>
      <Id>3</Id>
      <SpeakerId>3</SpeakerId>
