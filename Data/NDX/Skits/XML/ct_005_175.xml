    <Entry>
      <PointerOffset>37572</PointerOffset>
      <VoiceId>&lt;VCT_005_175_001&gt;</VoiceId>
      <JapaneseText>はぐはぐはぐ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>13</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38144</PointerOffset>
      <VoiceId>&lt;VCT_005_175_002&gt;</VoiceId>
      <JapaneseText>また食ってんのか、おまえ。
さっき昼飯食ったばっかだろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39020</PointerOffset>
      <VoiceId>&lt;VCT_005_175_003&gt;</VoiceId>
      <JapaneseText>これはデザートなの、ね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>13</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39636</PointerOffset>
      <VoiceId>&lt;VCT_005_175_004&gt;</VoiceId>
      <JapaneseText>そうそう、甘いものはお昼ご飯とは
別のところに入るんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>24</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40684</PointerOffset>
      <VoiceId>&lt;VCT_005_175_005&gt;</VoiceId>
      <JapaneseText>だから、少しくらいおなかいっぱいでも
食べられるんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>24</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41408</PointerOffset>
      <VoiceId>&lt;VCT_005_175_006&gt;</VoiceId>
      <JapaneseText>食えるとか食えないとかってよりさあ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41520</PointerOffset>
      <VoiceId>&lt;VCT_005_175_007&gt;</VoiceId>
      <JapaneseText>……また体重増えるぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42000</PointerOffset>
      <VoiceId>&lt;VCT_005_175_008&gt;</VoiceId>
      <JapaneseText>……あ、いってぇ……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43660</PointerOffset>
      <VoiceId>&lt;VCT_005_175_009&gt;</VoiceId>
      <JapaneseText>だから、それは繊細な乙女には
禁句だってば</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>22</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
