    <Entry>
      <PointerOffset>37360</PointerOffset>
      <VoiceId>&lt;VCT_004_167_001&gt;</VoiceId>
      <JapaneseText>やった！　やったぜ、ついに！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38024</PointerOffset>
      <VoiceId>&lt;VCT_004_167_002&gt;</VoiceId>
      <JapaneseText>クルルン！　クルルン！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>14</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38364</PointerOffset>
      <VoiceId>&lt;VCT_004_167_003&gt;</VoiceId>
      <JapaneseText>すごいね、&lt;DIO&gt;！
全コスチュームをマスターするなんて！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>13</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38960</PointerOffset>
      <VoiceId>&lt;VCT_004_167_004&gt;</VoiceId>
      <JapaneseText>へへ〜〜〜ん、オレに掛かりゃ、こんなもんだっての</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39476</PointerOffset>
      <VoiceId>&lt;VCT_004_167_005&gt;</VoiceId>
      <JapaneseText>見たか！　このタンス野郎が！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40288</PointerOffset>
      <VoiceId>&lt;VCT_004_167_006&gt;</VoiceId>
      <JapaneseText>へん、すごすぎて、言葉もないってか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41024</PointerOffset>
      <VoiceId>&lt;VCT_004_167_007&gt;</VoiceId>
      <JapaneseText>ったく、ギャーギャーうるせーのがおるだ。
そったらことで大騒ぎするなんて、器が小せぇ証拠だど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>25</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42788</PointerOffset>
      <VoiceId>&lt;VCT_004_167_008&gt;</VoiceId>
      <JapaneseText>おらだったら、サラッと流すだーよ。
大人っていうのは、そういうもんだ、ふん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>25</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43924</PointerOffset>
      <VoiceId>&lt;VCT_004_167_009&gt;</VoiceId>
      <JapaneseText>へん！　くやしくてしょうがねーって、
顔に書いてあんぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44528</PointerOffset>
      <VoiceId>&lt;VCT_004_167_010&gt;</VoiceId>
      <JapaneseText>だ！　え？　ど、ど、どったらとこに、書いてあるだ！
け、け、け、消す、今すぐ消すだ！　早く、早く！　ど、どこ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>25</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
