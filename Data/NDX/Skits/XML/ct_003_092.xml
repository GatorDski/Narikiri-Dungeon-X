    <Entry>
      <PointerOffset>37208</PointerOffset>
      <VoiceId>&lt;VCT_003_092_001&gt;</VoiceId>
      <JapaneseText>フラムベルクさんとの約束、
果たせなかった……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>1</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37320</PointerOffset>
      <VoiceId>&lt;VCT_003_092_002&gt;</VoiceId>
      <JapaneseText>このまま、オレら、
お宝泥棒の濡れ衣かぶったままかよ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>2</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37432</PointerOffset>
      <VoiceId>&lt;VCT_003_092_003&gt;</VoiceId>
      <JapaneseText>彼女はふたりが盗んだんじゃない
ってわかってんじゃないかな</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>3</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37980</PointerOffset>
      <VoiceId>&lt;VCT_003_092_004&gt;</VoiceId>
      <JapaneseText>え？　そうなの？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>4</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38280</PointerOffset>
      <VoiceId>&lt;VCT_003_092_005&gt;</VoiceId>
      <JapaneseText>あの時、フラムベルクは
『不凍の流体』の話してたでしょ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>5</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38524</PointerOffset>
      <VoiceId>&lt;VCT_003_092_006&gt;</VoiceId>
      <JapaneseText>盗んだ相手にもうひとつの宝物のありかを
教えたりするかな</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>6</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38756</PointerOffset>
      <VoiceId>&lt;VCT_003_092_007&gt;</VoiceId>
      <JapaneseText>それじゃあ、やっぱり</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>7</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39132</PointerOffset>
      <VoiceId>&lt;VCT_003_092_008&gt;</VoiceId>
      <JapaneseText>だったら、そう言ってくれりゃ
よかったのに……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>8</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39452</PointerOffset>
      <VoiceId>&lt;VCT_003_092_009&gt;</VoiceId>
      <JapaneseText>言ったら、君たちが『不燃の古木』を
取り返してくれないもん</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>9</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39884</PointerOffset>
      <VoiceId>&lt;VCT_003_092_010&gt;</VoiceId>
      <JapaneseText>オレら、
都合よく利用されたのかよ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>10</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
