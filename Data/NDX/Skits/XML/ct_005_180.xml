    <Entry>
      <PointerOffset>38100</PointerOffset>
      <VoiceId>&lt;VCT_005_180_002&gt;</VoiceId>
      <JapaneseText>&lt;MIN&gt;の好きなのは、リンゴ、だっけ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>15</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38400</PointerOffset>
      <VoiceId>&lt;VCT_005_180_003&gt;</VoiceId>
      <JapaneseText>え、ええ……
で、ですが、それは、ちょっと……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>17</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39172</PointerOffset>
      <VoiceId>&lt;VCT_005_180_004&gt;</VoiceId>
      <JapaneseText>……な、なんか焦げくさいぞ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>12</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39540</PointerOffset>
      <VoiceId>&lt;VCT_005_180_005&gt;</VoiceId>
      <JapaneseText>香ばしいの間違いでしょ？
これぞ、&lt;ARC&gt;特製フルーツ焼き！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>15</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41160</PointerOffset>
      <VoiceId>&lt;VCT_005_180_006&gt;</VoiceId>
      <JapaneseText>ヤバいぞ！　誰か水持ってこい！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>16</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42776</PointerOffset>
      <VoiceId>&lt;VCT_005_180_007&gt;</VoiceId>
      <JapaneseText>ふぅ……なんとか
延焼は防げたみたいだ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>16</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43124</PointerOffset>
      <VoiceId>&lt;VCT_005_180_008&gt;</VoiceId>
