    <Entry>
      <PointerOffset>37336</PointerOffset>
      <VoiceId>&lt;VCT_006_197_001&gt;</VoiceId>
      <JapaneseText>んっ……んん……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>1</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38068</PointerOffset>
      <VoiceId>&lt;VCT_006_197_002&gt;</VoiceId>
      <JapaneseText>どうしたの、かぜ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>2</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38372</PointerOffset>
      <VoiceId>&lt;VCT_006_197_003&gt;</VoiceId>
      <JapaneseText>わかねけど、ちょっと喉の調子が……
……ん、んっん……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>3</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39208</PointerOffset>
      <VoiceId>&lt;VCT_006_197_004&gt;</VoiceId>
      <JapaneseText>体調管理には気をつけないと</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>4</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39320</PointerOffset>
      <VoiceId>&lt;VCT_006_197_005&gt;</VoiceId>
      <JapaneseText>お〜い、チビ助ぇ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>5</Id>
      <UnknownPointer>25</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40356</PointerOffset>
      <VoiceId>&lt;VCT_006_197_006&gt;</VoiceId>
      <JapaneseText>んだよ！　また、屁掛けようってのか！
その手は二度と喰わねぇぞ！！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>6</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41276</PointerOffset>
      <VoiceId>&lt;VCT_006_197_007&gt;</VoiceId>
      <JapaneseText>こ、これって……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>7</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41704</PointerOffset>
      <VoiceId>&lt;VCT_006_197_008&gt;</VoiceId>
      <JapaneseText>もぎたての新鮮なフルーツだでよ。カゼの初期なら
十分なビタミン取って、サッサと寝れば治るだーよ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>8</Id>
      <UnknownPointer>25</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42648</PointerOffset>
      <VoiceId>&lt;VCT_006_197_009&gt;</VoiceId>
      <JapaneseText>&lt;DIO&gt;のために？
……でも、どうして……？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>9</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43288</PointerOffset>
      <VoiceId>&lt;VCT_006_197_010&gt;</VoiceId>
      <JapaneseText>細かいこたぁ、どうでもいいだど。
それ食って、とっとと寝れ！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>10</Id>
      <UnknownPointer>25</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44036</PointerOffset>
      <VoiceId>&lt;VCT_006_197_011&gt;</VoiceId>
      <JapaneseText>……ふふ、悪く言う割に心配してるんだ♪</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>11</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44860</PointerOffset>
      <VoiceId>&lt;VCT_006_197_012&gt;</VoiceId>
      <JapaneseText>ほら、&lt;DIO&gt;も早くそれ食べて休んだラ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>12</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45460</PointerOffset>
      <VoiceId>&lt;VCT_006_197_013&gt;</VoiceId>
      <JapaneseText>……いや、あの、その……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>13</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45692</PointerOffset>
      <VoiceId>&lt;VCT_006_197_014&gt;</VoiceId>
      <JapaneseText>サ……サンキュー……タン……アルベルト</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>14</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>46668</PointerOffset>
      <VoiceId>&lt;VCT_006_197_015&gt;</VoiceId>
      <JapaneseText>あの子ったら照れちゃって</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>15</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47092</PointerOffset>
      <VoiceId>&lt;VCT_006_197_016&gt;</VoiceId>
      <JapaneseText>……アルベルトも優しいところ、
あるんだね！</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>16</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47532</PointerOffset>
      <VoiceId>&lt;VCT_006_197_017&gt;</VoiceId>
      <JapaneseText>あ〜ん？　誰が優しいって？　バカこくでねーよ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>17</Id>
      <UnknownPointer>25</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>48120</PointerOffset>
      <VoiceId>&lt;VCT_006_197_018&gt;</VoiceId>
      <JapaneseText>え？　だ、だって、あのフルーツって、
&lt;DIO&gt;のために、わざわざ取ってきたんでしょ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>18</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>48540</PointerOffset>
      <VoiceId>&lt;VCT_006_197_019&gt;</VoiceId>
      <JapaneseText>昨日の食材の残りだ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>19</Id>
      <UnknownPointer>25</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>48936</PointerOffset>
      <VoiceId>&lt;VCT_006_197_020&gt;</VoiceId>
      <JapaneseText>じゃあ、早く寝ろって、
&lt;DIO&gt;の体調を気遣ってじゃ？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>20</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49304</PointerOffset>
      <VoiceId>&lt;VCT_006_197_021&gt;</VoiceId>
      <JapaneseText>あいつがカゼ引こうが、おらには関係ねぇだーよ！
&lt;MEL&gt;&lt;MEL&gt;に感染ると迷惑だで、サッサと治させたいだけだ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>21</Id>
      <UnknownPointer>25</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>50220</PointerOffset>
      <VoiceId>&lt;VCT_006_197_022&gt;</VoiceId>
      <JapaneseText>&lt;DIO&gt;には黙っておかなきゃ……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>22</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
