      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>3</Id>
      <UnknownPointer>16</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39252</PointerOffset>
      <VoiceId>&lt;VCT_002_082_004&gt;</VoiceId>
      <JapaneseText>&lt;CST&gt;さんまで……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>4</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
