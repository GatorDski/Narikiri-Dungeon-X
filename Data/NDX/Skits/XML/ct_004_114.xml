    <Entry>
      <PointerOffset>37440</PointerOffset>
      <VoiceId>&lt;VCT_004_114_001&gt;</VoiceId>
      <JapaneseText>うーん</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>1</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37592</PointerOffset>
      <VoiceId>&lt;VCT_004_114_002&gt;</VoiceId>
      <JapaneseText>？　どうしたの、&lt;DIO&gt;</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>2</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37900</PointerOffset>
      <VoiceId>&lt;VCT_004_114_003&gt;</VoiceId>
      <JapaneseText>いやさ……なんだかオレら、
スゲー強くなった、と思うんだけど……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>3</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38484</PointerOffset>
      <VoiceId>&lt;VCT_004_114_004&gt;</VoiceId>
      <JapaneseText>うん、今ならわたしも実感できるかも</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>4</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38740</PointerOffset>
      <VoiceId>&lt;VCT_004_114_005&gt;</VoiceId>
      <JapaneseText>……その割に&lt;DIO&gt;はしゃがないんだね、
前みたいに</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>5</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39216</PointerOffset>
      <VoiceId>&lt;VCT_004_114_006&gt;</VoiceId>
      <JapaneseText>うん……強いってのはコレだけじゃない
……気がすんだよな……よくわかんねけど</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>6</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39864</PointerOffset>
      <VoiceId>&lt;VCT_004_114_007&gt;</VoiceId>
      <JapaneseText>そう、かな……うん、そうだね</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>7</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40544</PointerOffset>
      <VoiceId>&lt;VCT_004_114_008&gt;</VoiceId>
      <JapaneseText>ただ速いとか腕力があるだけって
本当の強さじゃないかも</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>8</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41008</PointerOffset>
      <VoiceId>&lt;VCT_004_114_009&gt;</VoiceId>
      <JapaneseText>だろ？　なんつーか、オレらは
『なりきり士』なワケで……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>9</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41772</PointerOffset>
      <VoiceId>&lt;VCT_004_114_010&gt;</VoiceId>
      <JapaneseText>そっか！　いろんな職業に
なりきりれてこその強さも重要だよね</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>10</Id>
      <UnknownPointer>13</UnknownPointer>
      <SpeakerId>2</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42656</PointerOffset>
      <VoiceId>&lt;VCT_004_114_011&gt;</VoiceId>
      <JapaneseText>えーとじゃあどーすりゃいいんだ？つまり……</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>11</Id>
      <UnknownPointer>12</UnknownPointer>
      <SpeakerId>1</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43692</PointerOffset>
      <VoiceId>&lt;VCT_004_114_012&gt;</VoiceId>
      <JapaneseText>どう？　手がかかんなくなって、寂しい？</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>12</Id>
      <UnknownPointer>22</UnknownPointer>
      <SpeakerId>3</SpeakerId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44028</PointerOffset>
      <VoiceId>&lt;VCT_004_114_013&gt;</VoiceId>
      <JapaneseText>そう、だね……でもそれと同じくらい……
うれしくもあるんだ</JapaneseText>
      <EnglishText></EnglishText>
      <Notes></Notes>
      <Id>13</Id>
      <UnknownPointer>24</UnknownPointer>
      <SpeakerId>4</SpeakerId>
      <Status>To Do</Status>
    </Entry>
