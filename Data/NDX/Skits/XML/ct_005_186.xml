    <Entry>
      <PointerOffset>37388</PointerOffset>
      <VoiceId>&lt;VCT_005_186_001&gt;</VoiceId>
      <JapaneseText>最近、&lt;SUZ&gt;ちゃん、食欲ないけど平気？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>13</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37856</PointerOffset>
      <VoiceId>&lt;VCT_005_186_002&gt;</VoiceId>
      <JapaneseText>あっ、忘れてた！
&lt;SUZ&gt;ちゃん辛いもの苦手なんだっけ！？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>15</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38216</PointerOffset>
      <VoiceId>&lt;VCT_005_186_003&gt;</VoiceId>
      <JapaneseText>そっか……ここんとこ
カレー続きだったから……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer>13</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38748</PointerOffset>
      <VoiceId>&lt;VCT_005_186_004&gt;</VoiceId>
      <JapaneseText>それで、今日は甘口と辛口、
ふた種類作ってみました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>17</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39908</PointerOffset>
      <VoiceId>&lt;VCT_005_186_005&gt;</VoiceId>
      <JapaneseText>もっと早く気付けばよかったんですが……
……ごめんなさい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>17</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40512</PointerOffset>
      <VoiceId>&lt;VCT_005_186_006&gt;</VoiceId>
      <JapaneseText>そういうことではありません。最近、少し体調が
よくなくて、食欲がなかっただけです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>20</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41284</PointerOffset>
      <VoiceId>&lt;VCT_005_186_007&gt;</VoiceId>
      <JapaneseText>辛いのだって平気です</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>20</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41672</PointerOffset>
      <VoiceId>&lt;VCT_005_186_008&gt;</VoiceId>
      <JapaneseText>え、でも……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>17</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41944</PointerOffset>
      <VoiceId>&lt;VCT_005_186_009&gt;</VoiceId>
      <JapaneseText>これくらいの辛さに耐えられないようでは
忍びとしての修行が足りません</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>20</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42580</PointerOffset>
      <VoiceId>&lt;VCT_005_186_010&gt;</VoiceId>
      <JapaneseText>&lt;SUZ&gt;ちゃん、食べない方が……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer>17</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42812</PointerOffset>
      <VoiceId>&lt;VCT_005_186_011&gt;</VoiceId>
      <JapaneseText>んっ……！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <SpeakerId>4</SpeakerId>
      <UnknownPointer>20</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44580</PointerOffset>
      <VoiceId>&lt;VCT_005_186_012&gt;</VoiceId>
      <JapaneseText>きゃあっ！　&lt;SUZ&gt;ちゃん倒れちゃった！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer>15</UnknownPointer>
      <Status>To Do</Status>
    </Entry>
