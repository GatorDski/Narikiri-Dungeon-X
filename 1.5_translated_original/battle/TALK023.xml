      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>52</PointerOffset>
      <JapaneseText>ほら！　怒っちゃったよ！</JapaneseText>
      <EnglishText/>
