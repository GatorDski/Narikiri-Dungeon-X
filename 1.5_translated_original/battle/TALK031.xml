    <Entry>
      <PointerOffset>4</PointerOffset>
      <JapaneseText>我に刃向かうとは。
生きては帰さぬ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>20</PointerOffset>
      <JapaneseText>んなの横暴だ！
勝手に決めつけやがって！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>36</PointerOffset>
      <JapaneseText>私たちの話も聞いてください</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>52</PointerOffset>
      <JapaneseText>もはや聞くに値はせぬ、
覚悟しろ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>68</PointerOffset>
      <JapaneseText>相変わらず熱いやつだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>84</PointerOffset>
      <JapaneseText>力でわからせてやろうぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>100</PointerOffset>
      <JapaneseText>また戦うのですね……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
