    <Entry>
      <PointerOffset>261596</PointerOffset>
      <VoiceId>VSM_320_030_009</VoiceId>
      <JapaneseText>戦争がなかったら
オレは軍人には
ならなかった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>5</Id>
