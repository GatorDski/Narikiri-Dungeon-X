    <Entry>
      <PointerOffset>260608</PointerOffset>
      <VoiceId>VSM_320_030_003</VoiceId>
      <JapaneseText>……くそっ……
何やってんだよ……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>260608</PointerOffset>
      <VoiceId>VSM_320_030_004</VoiceId>
      <JapaneseText>そんな連中、
オレがいれば、
全滅させてやるのに……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
