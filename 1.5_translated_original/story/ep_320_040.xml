      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>261652</PointerOffset>
      <VoiceId>VSM_320_040_008</VoiceId>
      <JapaneseText>そして
魔科学兵器を作ったんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
